{
  "manifest_version": 3,
  "name": "HubsonWsad",
  "version": "1.0",
  "description": "Coś wymyślimy",
  "permissions": ["activeTab", "scripting", "tabs", "storage"],
  "host_permissions": [
    "https://potentai.pl/*"
  ],
  "action": {
    "default_title": "Pokaż/Ukryj Panel Boczny"
  },
  "background": {
    "service_worker": "background.js"
  },
  "web_accessible_resources": [
    {
      "resources": ["sidebar.html", "styles.css", "script.js", "fillForm.js"],
      "matches": ["<all_urls>"]
    }
  ]
}
